{
  "manifest_version": 3,
  "name": "Open with mobile",
  "description": "Open your website on your mobile phone",
  "version": "1.2",
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon128.png",
    "48": "icon128.png",
    "128": "icon128.png"
  }
}