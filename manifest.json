{
  "manifest_version": 2,
  "name": "Open with mobile",
  "description": "Open your website on your mobile phone",
  "version": "1.1",
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "icon.jpg",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.jpg",
    "48": "icon48.jpg",
    "128": "icon128.jpg"
  }
}
